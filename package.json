{
  "name": "@maplat/tin",
  "version": "0.6.7",
  "description": "JavaScript library which performs homeomorphic conversion mutually between the coordinate systems of two planes based on the control points.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib/",
    "src/",
    "www/"
  ],
  "scripts": {
    "www_build": "webpack",
    "npm_build": "babel src/ --out-dir lib/ --source-maps",
    "test": "jest",
    "prepublish": "npm run lint && npm run test && npm run www_build && npm run npm_build",
    "preinstall": "npx npm-force-resolutions || echo 'ignore'",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.{js,ts}\" --write",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/MaplatTin.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/code4history/MaplatTin/issues"
  },
  "homepage": "https://github.com/code4history/MaplatTin/wiki",
  "dependencies": {
    "@turf/turf": "^5.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-matcher-deep-close-to": "^2.0.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
